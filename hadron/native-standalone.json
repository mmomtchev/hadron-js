{
  "include": "./native.json",
  "buildConfigurations": {
    "native": {
      "actions": {
        "clang": "clang"
      },
      "properties": {
        "buildType": "release",
        "toolchain": "clang"
      },
      "devDependencies": {
        "@xpack-dev-tools/clang": "17.0.6-1.1"
      }
    },
    "native-debug": {
      "inherit": [
        "native"
      ],
      "properties": {
        "buildType": "debug",
        "argsConanVerbose": "",
        "argsMesonVerbose": "--verbose"
      }
    }
  }
}
